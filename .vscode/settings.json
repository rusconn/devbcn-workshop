{
  "files.associations": {
    "*.sql": "postgres",
    "*.css": "tailwindcss"
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "rust-analyzer.check.command": "clippy",
  "tailwindCSS.experimental.classRegex": ["class: \"(.*)\""],
  "tailwindCSS.includeLanguages": {
    "rust": "html"
  }
}
